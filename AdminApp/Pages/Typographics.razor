@page "/typographics"
    <div class="main-content">

        <h3>Typographics</h3>

        <div className="typographics">

            <p className="h1">h1. SmallAdmin heading</p>
            <p className="h2">h2. SmallAdmin heading</p>
            <p className="h3">h3. SmallAdmin heading</p>
            <p className="h4">h4. SmallAdmin heading</p>
            <p className="h5">h5. SmallAdmin heading</p>
